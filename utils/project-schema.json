{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$comment": "DIAL Catalog Project Metaschema: JSON Schema",
  "title": "DIAL Projects",
  "type": "object",
  "required": [
    "name",
    "description",
    "origin",
    "start_date",
    "end_date",
    "sectors",
    "countries"
  ],
  "properties": {
    "name": {
      "type": "string",
      "title": "The name of the project"
    },
    "description": {
      "type": "string",
      "title": "The description of the project."
    },
    "start_date": {
      "type": "string",
      "format": "date",
      "title": "The start date of the project in ISO format."
    },
    "end_date": {
      "type": "string",
      "format": "date",
      "title": "The end date of the project in ISO format."
    },
    "origin": {
      "type": "string",
      "description": "List of origin of the project information.",
      "enum": [
        "Digital Health Atlas", 
        "DIAL OSC", 
        "Digital Public Goods Alliance", 
        "Digital Square", 
        "Digital Government Platform Tracker",
        "GIZ"
      ]
    },
    "products": {
      "type": "array",
      "description": "List of products being used in the project.",
      "uniqueItems": true
    },
    "sectors": {
      "type": "array",
      "description": "List of sectors for the project.",
      "uniqueItems": true,
      "items": {
        "type": "string",
        "enum": [
          "Environment and Climate",
          "Agriculture and Rural Development",
          "Security, Peace, Reconstruction",
          "Education and Social Development",
          "State and Democracy (Good Governance)",
          "Sustainable Infrastructure",
          "Work and Employment",
          "Economics and Finance",
          "Digital, Data, and Technology",
          "Health",
          "Private Sector Engagement",
          "Sustainable Development",
          "Other"
        ]
      }
    },
    "budget": {
      "type": "integer",
      "title": "The budget of the project."
    },
    "project_url": {
      "type": "string",
      "title": "A link to a URL/webpage for this project"
    },
