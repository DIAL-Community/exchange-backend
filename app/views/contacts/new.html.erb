<% if @contact.errors.any? %>
  <div id="error_explanation">
    <div id="error_explanation">
      <div class="alert alert-danger">
        The form contains
        <%= pluralize(@contact.errors.count, "error") %>.
      </div>
    </div>
  </div>
<% end %>
<div class="card">
  <h5 class="card-header">
    Create contact
  </h5>
  <%= render 'form', contact: @contact %>
</div>
