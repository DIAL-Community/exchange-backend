<% if @contact.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
        <%= t('view.general.error', count: @contact.errors.count) %>
    </div>
  </div>
<% end %>
<h5 class="border rounded bg-secondary text-white p-3">
  <%= t('view.general.title.edit', model: t('model.contact').to_s.titlecase) %>
</h5>
<%= render 'form', contact: @contact %>
