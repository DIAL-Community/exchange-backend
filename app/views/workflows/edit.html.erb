<% if @workflow.errors.any? %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      The form contains
      <%= pluralize(@workflow.errors.count, "error") %>.
    </div>
  </div>
<% end %>
<h5 class="border rounded bg-secondary text-white p-3">
  Edit workflow
</h5>
<%= render 'form', workflow: @workflow %>
